@model MVCCV.Models.Entity.TblReferences
@{
    ViewBag.Title = "GetExperience";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<br />
<h2>Referans Güncelleme Sayfası</h2>
<br />
<form method="post" class="form-group">
    @Html.Label("Referans Adı Soyadı ")
    @Html.TextBoxFor(x => x.NameSurname, new { @class = "form-control" })
    @Html.Label("Referans Ünvanı  ")
    @Html.TextBoxFor(x => x.Title, new { @class = "form-control" })
    @Html.Label("Referans Telefonu")
    @Html.TextBoxFor(x => x.Phone, new { @class = "form-control" })
   


    <button class="btn btn-info">Güncelle</button>
</form>
